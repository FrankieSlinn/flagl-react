{
  "expo": {
    "name": "flagl-react",
    "slug": "flagl-react",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/FLAGL_FAV_1024.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#ffffff",
      "image": "./assets/images/GLOBE_SPLASH.jpg",
      "resizeMode": "contain"
     
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.frankissling.flagl",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/flagImage.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "01a24365-50c0-4293-9b94-f1b0ffd2a6f7"
      }
    }
  }
}
